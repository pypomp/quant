#!/bin/bash
#SBATCH --job-name="SPX pfilter check (R pomp)"
#SBATCH --output=R_results/logs/slurm-%j.out
#SBATCH --mail-type=FAIL
#SBATCH --mail-type=END
#SBATCH --mail-user=aaronabk@umich.edu
#SBATCH --nodes=1
#SBATCH --ntasks-per-node=36
#SBATCH --cpus-per-task=1
#SBATCH --mem-per-cpu=2GB
#SBATCH --account="ionides0"
#SBATCH -p standard

R_file="eval.R"
module load R/4.4.0

# Useful for debugging:
echo "Running on $SLURM_JOB_NODELIST"
echo "Running in $(pwd)"
echo "Using run level $run_level"
 
### R commands ###
echo "Running spx/pfilter_check/$R_file..."
R CMD BATCH --no-restore --no-save \
  spx/pfilter_check/$R_file spx/pfilter_check/R_results/logs/$R_file"out"

